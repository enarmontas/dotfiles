[user]
  name = Ernestas Narmontas
  email = (none)
[alias]
  st = status
  ci = commit
  co = checkout
  br = branch
[color]
  diff = auto
  status = auto
  branch = auto
  ui = true
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[branch]
  autosetuprebase = always
[branch "master"]
  rebase = true
[branch "main"]
  rebase = true
[rebase]
  stat = true
[push]
  default = current
[web]
  browser = open
[core]
  excludesfile = ~/.gitignore
  editor = vim
[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
